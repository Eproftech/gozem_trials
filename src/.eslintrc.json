{
    "parserOptions": {
        "env": {
            "browser": false,
            "commonjs": true,
            "es6": true,
            "node": true
        },
        "parseOptions": {
            "ecmaFeatures": {
                "jsx": true
            },
            "sourceType": "module"
        },
        "rules": {
            "no-const-assign": "warn",
            "no-this-before-super": "warn",
            "no-undef": "error",
            "no-unreachable": "error",
            "no-unused-vars": "warn",
            "constructor-super": "warn",
            "valid-typeof": "warn"
        }
    },
    "parser": "babel-eslint",
    "extends": ["eslint:recommended"]
}